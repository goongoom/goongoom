  "name": "goongoom",
  "version": "1.0.0",
  "private": false,
  "packageManager": "bun@1.2.19",
  "scripts": {
    "dev": "concurrently \"next dev\" \"convex dev --local\"",
    "dev:proddata": "bun --env-file=.env --env-file=.env.proddata next dev",
    "build": "next build",
    "deploy": "convex deploy --cmd 'next build'",
